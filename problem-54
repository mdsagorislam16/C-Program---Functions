#include <stdio.h>
#include <math.h>

int main() {
    double a, b, c, d, r1, r2;
    scanf("%lf %lf %lf", &a, &b, &c);

    d = b*b - 4*a*c;

    if(d > 0) {
        r1 = (-b + sqrt(d)) / (2*a);
        r2 = (-b - sqrt(d)) / (2*a);
        printf("Root1 = %.2lf\nRoot2 = %.2lf\n", r1, r2);
    }
    else if(d == 0) {
        r1 = -b / (2*a);
        printf("Root = %.2lf\n", r1);
    }
    else {
        double real = -b / (2*a);
        double imag = sqrt(-d) / (2*a);

        printf("Root1 = %.2lf + %.2lfi\n", real, imag);
        printf("Root2 = %.2lf - %.2lfi\n", real, imag);
    }

    return 0;
}

/*
Input:
1 5 6

Output:
Root1 = -2.00
Root2 = -3.00
*/
