#include <stdio.h>

void insertBottom(int stack[], int *top, int item) {
    int temp;

    if(*top == -1) {
        stack[++(*top)] = item;
        return;
    }

    temp = stack[(*top)--];
    insertBottom(stack, top, item);
    stack[++(*top)] = temp;
}

void reverse(int stack[], int *top) {
    int item;
    if(*top == -1) return;

    item = stack[(*top)--];
    reverse(stack, top);
    insertBottom(stack, top, item);
}

int main() {
    int stack[100], n, i, top = -1;

    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        scanf("%d", &stack[++top]);
    }

    reverse(stack, &top);

    for(i = 0; i <= top; i++) {
        printf("%d ", stack[i]);
    }

    return 0;
}

/*
Input:
5
1 2 3 4 5

Output:
5 4 3 2 1
*/
